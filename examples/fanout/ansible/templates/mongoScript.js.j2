db = new Mongo().getDB("{{ db_name }}")
for (let i = 1; i <= {{ count }}; i++){
  db.createCollection(`{{ db_name }}${i}`, { capped: false })
  let entries = []
  for (let i = 0; i <= 50; i++){
    entries.push({key: i, value: Math.floor(Math.random() * 50)})
  }
    db[`{{ db_name }}${i}`].insertMany(entries)
}


