// This proto contains gRPC definition for chain_x service
syntax = "proto3";

package chain_{{ chain_x }};

option go_package = "github.com/hanapedia/microservice-topologies/chain/chain_{{ chain_x }}/pb_chain_{{ chain_x }}";

service Chain_{{ chain_x }} {
  rpc GetIds(Req) returns (Res); 
  }

message Res {
    repeated int32 ids = 1;
  }

message Req {
    repeated int32 ids = 1;
  }

